{
  "expo": {
    "name": "Fitway",
    "slug": "Fitway",
    "scheme": "fitway",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#131313"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#131313"
      },
      "package": "com.juanchiigomezz.fitway",
      "softwareKeyboardLayoutMode": "pan"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow Fitway to access camera."
        }
      ],
      "expo-localization",
      "@react-native-google-signin/google-signin",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow Fitway to access your camera."
        }
      ]
    ],

    "extra": {
      "eas": {
        "projectId": "8129cdd7-4d9c-4bc0-b14c-6e7326beb3fa"
      }
    },
    "owner": "juanchiigomezz"
  }
}
